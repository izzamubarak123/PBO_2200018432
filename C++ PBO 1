#include <iostream>
using namespace std;

int main() {
    int angka = 5;

    for (int i = 0; i < angka; i++) {
        if (i % 2 == 0) {
            cout << i << " adalah angka genap." << endl;
        } else {
            cout << i << " adalah angka ganjil." << endl;
        }
    }

    int j = 0;
    while (j < angka) {
        j++;
    }

    int k = 0;
    do {
        k++;
    } while (k < angka);

    int arr[5] = {1, 2, 3, 4, 5};

    int mat[3][3] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};

    int input;
    cout << "Masukkan sebuah angka: ";
    cin >> input;

    return 0;
}
